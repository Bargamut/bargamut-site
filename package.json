{
  "name": "bargamutru",
  "version": "0.1.0",
  "description": "Мой персональный сайт",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "rollup -c",
    "build::webpack": "webpack",
    "dev": "rollup -c -w",
    "start": "sirv public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bargamut/bargamutru"
  },
  "author": {
    "name": "Paul Petrov",
    "email": "paul@bargamut.ru",
    "url": "https://bargamut.ru"
  },
  "private": true,
  "license": "UNLICENSED",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/libs/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "cache": true,
    "all": true
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "autoprefixer": "10.4.13",
    "coveralls": "^3.0.2",
    "css-loader": "6.7.1",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "postcss": "8.4.18",
    "postcss-load-config": "4.0.1",
    "postcss-loader": "7.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-loader": "3.1.3",
    "svelte-preprocess": "4.10.7",
    "typescript": "^3.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "sirv-cli": "^0.4.4"
  }
}
