{
  "name": "bargamutru",
  "version": "1.0.0",
  "description": "Мой персональный сайт",
  "main": "main.tsx",
  "scripts": {
    "test": "npm run eslint && nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "eslint": "eslint --ext .js src/service",
    "build": "webpack",
    "start": "webpack serve --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bargamut/bargamutru"
  },
  "author": {
    "name": "Paul Petrov",
    "email": "paul@bargamut.ru",
    "url": "https://bargamut.ru"
  },
  "private": true,
  "license": "UNLICENSED",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/libs/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "cache": true,
    "all": true
  },
  "devDependencies": {
    "@types/express-serve-static-core": "4.17.30",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-redux": "7.1.24",
    "autoprefixer": "10.4.13",
    "coveralls": "^3.0.2",
    "css-loader": "6.7.1",
    "eslint": "8.26.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "mocha": "^10.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "ts-loader": "9.4.1",
    "typescript": "^4.9.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.1",
    "bootstrap": "4.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5"
  }
}
